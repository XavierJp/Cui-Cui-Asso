<div class="paragraphs">
  <div class="paragraph">
    <div class="text-container">
      {% markdown introBlock %}
    </div>
  </div>
  {%- for post in blocks -%}
    <div class="paragraph">
      <div class="text-container">
        <h2 id="{{ post.menuTitle | defaultStr | slugify }}" class="inlined-title">{{ post.title }}</h2>
        <br/>
        {% if post.medaillon %}
          <div class="flex-row">
            <div>{% markdown post.body %}</div>
            <img class="fanzine-medaillon" src="{{ post.medaillon }}"/>
          </div>
        {% else %}
          <div>{% markdown post.body %}</div>
        {% endif %}
        {% if post.link %}
          <div class="center button">
            <div>
              <a class="dont-style" href="{{ post.link }}">
                ☞ Commander Cui-Cui
              </a>
            </div>
          </div>
        {% endif %}
        {% if post.separator %}
          <div class="img-separator">
            {% include post.separator %}
          </div>
        {% endif %}
      </div>
    </div>
  {%- endfor -%}
</div>